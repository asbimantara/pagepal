{
    "name": "user/book",
    "description": "PagePal - Aplikasi tracking buku",
    "type": "project",
    "require": {
        "php": "^8.2",
        "mongodb/mongodb": "^1.20",
        "vlucas/phpdotenv": "^5.6",
        "ext-mongodb": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
